    "**/*.idx",
    "**/*.ind",
    "**/*.lof",
    "**/*.lot",
    "**/*.out",
    "**/*.toc",
    "**/*.acn",
    "**/*.acr",
    "**/*.alg",
    "**/*.glg",
    "**/*.glo",
    "**/*.gls",
    "**/*.ist",
    "**/*.fls",
    "**/*.log",
    "**/*.nav",
    "**/*.snm",
    "**/*.fdb_latexmk",
    "**/*.synctex.gz",
    "**/*.run.xml",
    "**/*_temp.tex"
  ],
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.snm",
    "*.synctex(busy)",
    "*.synctex.gz(busy)",
    "*.nav",
    "*_temp.tex"
  ],
  "[html]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "ruby.lint": {
    "rubocop": false
  },
  "[ruby]": {
    "editor.formatOnSave": false
  },
  "[css]": {
    "editor.defaultFormatter": "bradlc.vscode-tailwindcss"
  },
  "python.envFile": "${workspaceFolder}/nbenv",
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4 }",
  "eslint.packageManager": "yarn",
  "eslint.alwaysShowStatus": true,
  "go.useLanguageServer": true,
  "C_Cpp.updateChannel": "Insiders",
  "ruby.rubocop.useBundler": false,
  "[javascriptreact]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "explorer.confirmDelete": false,
  "dart.debugExternalLibraries": false,
  "dart.debugSdkLibraries": false,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "scssFormatter.singleQuote": true,
  "css.validate": false,
  "scss.validate": false,
  "stylelint.enable": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.format.enable": true,
  "latex-workshop.latex.autoClean.run": "onBuilt",
  "latex-workshop.latex.recipes": [
    {
      "name": "latexmk ðŸ”ƒ",
      "tools": [
        "latexmk"
      ]
    },
    {
      "name": "latexmk (lualatex)",
      "tools": [
        "lualatexmk"
      ]
    },
    {
      "name": "latexmk (latexmkrc)",
      "tools": [
        "latexmk_rconly"
      ]
    },
    {
      "name": "pdflatex âžž bibtex âžž pdflatexâ€ŠÃ—â€Š2",
      "tools": [
        "pdflatex",
        "bibtex",
        "pdflatex",
        "pdflatex"
      ]
    },
    {
      "name": "Compile Rnw files",
      "tools": [
        "rnw2tex",
        "latexmk"
      ]
    },
    {
      "name": "Compile Jnw files",
      "tools": [
        "jnw2tex",
        "latexmk"
      ]
    },
    {
      "name": "tectonic",
      "tools": [
        "tectonic"
      ]
    }
  ],
  "[latex]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.suggestSelection": "recentlyUsedByPrefix"
  },
  "git.confirmSync": false,
  "solargraph.checkGemVersion": false,
  "solargraph.useBundler": true,
  "solargraph.commandPath": "/Users/sangam/.rbenv/shims/solargraph",
  "ruby.useBundler": true,
  "ruby.useLanguageServer": true,
  "ruby.intellisense": "rubyLocate",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb",
    "*.svg": "default"
  },
  "dart.previewLsp": true,
  "[yaml]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "flow.pathToFlow": "${workspaceFolder}/node_modules/.bin/flow",
  "tabnine.experimentalAutoImports": true,
  "svg.preview.mode": "svg",
  "security.workspace.trust.untrustedFiles": "open",
  "[cpp]": {
    "editor.wordBasedSuggestions": false,
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true
  },
  "python.defaultInterpreterPath": "/Users/sangam/.asdf/shims/python3",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "dart.debugExternalPackageLibraries": true,
  "explorer.confirmDragAndDrop": false,
  "[python]": {
    "editor.tabSize": 4,
    "gitlens.codeLens.symbolScopes": [
      "!Module"
    ],
    "editor.wordBasedSuggestions": false,
    "editor.formatOnType": true
  },
  "editor.guides.indentation": true,
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--fast"
  ]
